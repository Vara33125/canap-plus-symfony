<!DOCTYPE html>

<title>Store</title>

{% block body %}
    <h1>Store</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ store.id }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ store.picture }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ store.adresse }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ store.ville }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ store.cp }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_store_index') }}">back to list</a>

    <a href="{{ path('app_admin_store_edit', {'id': store.id}) }}">edit</a>

    {{ include('admin_store/_delete_form.html.twig') }}
{% endblock %}
